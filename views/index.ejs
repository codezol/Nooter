<!DOCTYPE html>
<html lang="en">
   <%- include('partials/head.ejs') %>
<body>
   <%- include('./partials/nav.ejs') %>
   <main>
      <div class="container">

   <% if(notes.length > 0) { %>
   <% notes.forEach(note => { %>
      <a href="/notes/<%= note.id %>">
      <div class="note">
         <h3><%= note.title  %></h3>
         <p><%= note.body %></p>
      </div>
   </a>
   <% }) %>  
   <% } %>

</div>
</main>
<%- include('./partials/footer.ejs') %>
</body>
</html>
